{
    "markdown-preview-enhanced.chromePath": "/usr/bin/google-chrome",
    "settingsSync.ignoredSettings": [
        "-markdown-preview-enhanced.chromePath"
    ],
    "svelte.enable-ts-plugin": true,
    "vim.normalModeKeyBindings": [
        {
            "before": ["leader", "s"],
            "after": ["<C-v>"]
        },
        {

        }
    ],
    "vim.visualModeKeyBindings": [
        {
            "before": [
                ">"
            ],
            "commands": [
                "editor.action.indentLines"
            ]
        },
        {
            "before": [
                "<"
            ],
            "commands": [
                "editor.action.outdentLines"
            ]
        }
    ],
    "vim.insertModeKeyBindings": [
        {
        "before": ["j", "j"],
        "after": ["<Esc>"]
        }
    ],
    "vim.handleKeys": {
        // copy and paste
        "<C-v>": false,
        "<C-c>": false,
        "<C-x>": false,
        // find/select all
        "<C-f>": false,
        "<C-a>": false,
        // vs code new marker @ next
        "<C-d>": false,
        // undo/redo
        "<C-z>": false,
        "<C-y>": false
    }
}